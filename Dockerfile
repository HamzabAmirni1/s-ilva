FROM quay.io/sylivanu/s-ilva
RUN git clone https://github.com/Sylivanu/s-ilva.git /root/silva
WORKDIR /root/silva/
RUN rm -rf .git
RUN npm install
EXPOSE 8000
CMD ["npm", "start"]































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































